{
  "name": "code-linting-configs",
  "version": "1.0.0",
  "description": "simple code linting configs and demo",
  "scripts": {
    "esformat": "babel-node bin/esformatter.js",
    "jsformat": "node bin/jsBeautify.js",
    "jslint": "eslint beautify_demo/after.js --config .es5.eslintrc",
    "jslint-fix": "eslint --fix beautify_demo/after.js --config .es5.eslintrc",
    "lessformat": "node bin/lessBeautify.js",
    "lesslint": "lesshint beautify_demo/after.less",
    "stylint": "stylint beautify_demo/style.styl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/luoyi/code-linting-configs.git"
  },
  "keywords": [
    "code",
    "lint"
  ],
  "author": "luoyi",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/luoyi/code-linting-configs/issues"
  },
  "homepage": "https://gitlab.com/luoyi/code-linting-configs#README",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "esformatter": "^0.9.6",
    "esformatter-braces": "^1.2.1",
    "esformatter-curly": "0.0.1",
    "esformatter-jsx": "^7.0.1",
    "eslint": "^3.2.0",
    "eslint-config-airbnb-es5": "^1.0.9",
    "eslint-plugin-react": "^5.2.2",
    "glob": "^7.0.5",
    "js-beautify": "^1.6.3",
    "lesshint": "^2.0.2",
    "stylint": "^1.5.6"
  }
}
